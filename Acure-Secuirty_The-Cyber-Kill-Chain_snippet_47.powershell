# Define the MS Graph API endpoint
$graphApiEndpoint = "https://graph.microsoft.com/v1.0/"

# Fetch users
$users = Invoke-RestMethod -Uri "$graphApiEndpoint/users" -Headers $headers
$users.value

# Fetch Teams presence for each user
foreach ($user in $users.value) {
    $presence = Invoke-RestMethod -Uri "$graphApiEndpoint/users/$($user.id)/presence" -Headers $headers
    Write-Output "$($user.displayName) is $($presence.availability)"
}