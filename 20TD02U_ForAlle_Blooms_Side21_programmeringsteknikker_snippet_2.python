tall = 10

# If-setning
if tall > 5:
    print("Tallet er større enn 5")
    
# If-else-setning
if tall > 5:
    print("Tallet er større enn 5")
else:
    print("Tallet er 5 eller mindre")

# If-elif-else-setning
tall = 3
if tall > 5:
    print("Tallet er større enn 5")
elif tall == 5:
    print("Tallet er lik 5")
else:
    print("Tallet er mindre enn 5")