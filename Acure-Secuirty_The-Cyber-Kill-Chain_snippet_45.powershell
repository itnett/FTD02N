$urls = @(
    "https://example.sharepoint.com/sites",
    "https://example.sharepoint.com/teams",
    "https://teams.microsoft.com/l/example@domain.com"
)

foreach ($url in $urls) {
    try {
        $response = Invoke-WebRequest -Uri $url -Method Head -ErrorAction Stop
        if ($response.StatusCode -eq 200) {
            Write-Output "URL is accessible: $url"
        }
    } catch {
        Write-Output "URL is not accessible: $url"
    }
}